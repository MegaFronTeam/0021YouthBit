mixin sPersons(data)
	// start sPersons
	+b.SECTION.sPersons.section#sPersons&attributes(attributes)
		.container
			script
				include script.js
			+b.section-title
				h2 Наставники и&nbsp;Жюри
			mixin slide(t,p)
				+e.slide.swiper-slide
					+e.item
						+e.img-wrap
							+img("img/avatar.png")
						+e.name= t
						+e.text= p
			mixin control()
				.row
					.col
						.swiper-pagination
					.col-auto
						+arrow 
			+e.title Эксперты
			+e.slider--js.swiper#expert
				.swiper-wrapper
					//- - for (var x = 0; x < 2; x++)
					//- 	+slide('Виктор Сожможнов','Следует отметить, что высококачественный прототип будущего проекта способствует')
					//- 	+slide('Виктория Складчинкова','Прототип будущего проекта способствует')
					//- 	+slide('Ким Самаджокис','Следует отметить')
					//- 	+slide('Николай Ионов','Следует отметить, что высококачественный ')
				+control()
				
			//- +e.title Трекеры
			//- +e.slider--js.swiper#tracker
			//- 	.swiper-wrapper
					//- - for (var x = 0; x < 2; x++)
					//- 	+slide('Виктор Сожможнов','Следует отметить, что высококачественный прототип будущего проекта способствует')
					//- 	+slide('Виктория Складчинкова','Прототип будущего проекта способствует')
					//- 	+slide('Ким Самаджокис','Следует отметить')
					//- 	+slide('Николай Ионов','Следует отметить, что высококачественный ')
				+control()

			+e.title Жюри
			+e.slider--js.swiper.mb-0#jury
				.swiper-wrapper
					//- - for (var x = 0; x < 2; x++)
					//- 	+slide('Виктор Сожможнов','Следует отметить, что высококачественный прототип будущего проекта способствует')
					//- 	+slide('Виктория Складчинкова','Прототип будущего проекта способствует')
					//- 	+slide('Ким Самаджокис','Следует отметить')
					//- 	+slide('Николай Ионов','Следует отметить, что высококачественный ')
				+control()
	// end sPersons